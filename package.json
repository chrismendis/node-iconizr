{
	"name": "iconizr",
	"version": "1.0.0",
	"author": {
		"name": "Joschi Kuphal",
		"email": "joschi@kuphal.net",
		"url": "http://jkphl.is"
	},
	"description": "Takes a folder of SVG images and creates a CSS icon kit out of them, serving the icons as SVG / PNG sprite or embedded data URIs (depending on the client's capabilities), along with suitable CSS / Sass / LESS / Stylus etc. resources and a JavaScript loader for easy integration into your HTML documents",
	"homepage": "https://github.com/jkphl/node-iconizr",
	"repository": {
		"type": "git",
		"url": "https://github.com/jkphl/node-iconizr.git"
	},
	"bugs": {
		"url": "https://github.com/jkphl/node-iconizr/issues"
	},
	"licenses": [
		{
			"type": "MIT",
			"url": "https://github.com/jkphl/node-iconizr/blob/master/LICENSE.txt"
		}
	],
	"main": "lib/iconizr.js",
	"bin": {
		"iconizr": "./bin/node-iconizr.js"
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
		"coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
		"flowchart": "./node_modules/mermaid/bin/mermaid.js -o ./test/fixture/flowchart ./test/fixture/flowchart/logic"
	},
	"dependencies": {
		"mkdirp": "^0.5.0",
		"async": "^0.9.0",
		"lodash": "lodash/lodash",
		"vinyl": "^0.4.6",
		"phantomjs": "^1.9.12",
		"mustache": "^1.0.0",
		"svg-sprite": "^1.0.7",
		"winston": "^0.8.3",
	    "pngcrush-bin": "^2.2.0",
	    "pngquant-bin": "^2.0.3",
	    "optipng-bin": "^2.0.4",
	    "temp": "^0.8.1",
	    "js-yaml": "^3.2.6",
		"yargs": "^3.2.1",
		"prettysize": "^0.0.3",
		"uglify-js": "^2.4.16"
	},
	"devDependencies": {
		"mocha": "",
		"coveralls": "*",
		"mocha-lcov-reporter": "*",
		"istanbul": "*",
		"should": "",
		"rimraf": "",
		"glob": "^4.3.1",
		"mermaid": "^0.3.0"
	},
	"keywords": [
		"icon",
		"icons",
		"svg",
		"png",
		"sprite",
		"spritesheet",
		"stack",
		"generator",
		"css",
		"sass",
		"less",
		"stylus",
		"stylesheet",
		"inline",
		"html",
		"vector",
		"rwd",
		"retina",
		"mustache",
		"gulpfriendly"
	]
}